    "type": "library",
    "license": "MIT",
    "keywords": ["monolog", "pushbullet", "notification", "laravel"],
    "require": {
        "php": ">=5.4",
        "monolog/monolog": "^1.19",
        "joetannenbaum/phpushbullet": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Monobullet\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adnan RIHAN",
            "email": "adnan@rihan.fr"
        }
    ],
    "minimum-stability": "dev"
}
